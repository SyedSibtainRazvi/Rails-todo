<h1>Todo App</h1>
<table border>
  <thead>
    <tr>
      <th>Task Title</th>
      <th>Completed</th>
    </tr>
  </thead>
  <tbody>
    <% @todos.each do |todo|%>
      <tr>
        <td><%= todo.title%></td>
        <td><%= todo.completed ? "Yes" : "No"%></td>
        <td><%= link_to "Show Todo", todo%></td>
      </tr>
    </tbody>
  <%end%>
</table>